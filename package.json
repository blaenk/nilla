{
  "name": "nilla",
  "version": "0.0.1",
  "description": "An interface to rtorrent",
  "scripts": {
    "start": "node ./server/src/server.js",
    "webpack": "NODE_ENV=production webpack --progress --colors -p",
    "webpack-watch": "NODE_ENV=development webpack --progress --colors --watch -d",
    "test": "mocha server/test",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha server/test --report lcovonly -- -R spec && rm -rfv coverage/",
    "lint": "eslint --no-inline-config server/ client/",
    "doc": "documentation build -o docs/ -f html -g server/src/",
    "jsdoc": "jsdoc -d docs/ server/src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blaenk/nilla.git"
  },
  "keywords": [
    "rtorrent",
    "torrent"
  ],
  "author": "Jorge Israel Pe√±a",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blaenk/nilla/issues"
  },
  "homepage": "https://github.com/blaenk/nilla#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.1",
    "documentation": "^4.0.0-beta5",
    "file-loader": "^0.8.5",
    "hjs-webpack": "^8.2.0",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bencode": "^0.10.0",
    "bluebird": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "font-awesome": "^4.6.3",
    "lodash": "^4.13.1",
    "magnet-uri": "^5.1.3",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
    "semver": "^5.1.0",
    "xmlrpc": "blaenk/node-xmlrpc#custom-transport"
  }
}
