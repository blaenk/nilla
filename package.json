{
  "name": "nilla",
  "version": "0.0.1",
  "description": "An interface to rtorrent",
  "scripts": {
    "start": "node ./server/src/server.js",
    "lint": "eslint --no-inline-config server/ client/",
    "webpack": "NODE_ENV=production webpack --progress --colors -p",
    "webpack-watch": "NODE_ENV=development webpack --progress --colors --watch -d",
    "mocha": "NODE_ENV=test mocha server/test",
    "karma": "NODE_ENV=test karma start karma.conf.js",
    "test": "npm run mocha && npm run karma",
    "coverage": "istanbul cover --dir ./coverage/server ./node_modules/mocha/bin/_mocha server/test --report lcovonly -- -R spec",
    "doc": "documentation build -o docs/ -f html -g server/src/",
    "jsdoc": "jsdoc -d docs/ server/src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blaenk/nilla.git"
  },
  "keywords": [
    "rtorrent",
    "torrent"
  ],
  "author": "Jorge Israel Pe√±a",
